#include <iostream>
using namespace std;

int main() {
    int N;
    cin >> N;

    int vet[100];

    for (int i = 0; i < N; i++) {
        cin >> vet[i];
    }

    int cont = 0;

    for (int i = 0; i < N; i++) {
        if (vet[i] % 2 == 0) {
            cout << vet[i] << " ";
            cont++;
        }
    }

    cout << endl;
    cout << "Quantidade de pares = " << cont << endl;

    return 0;
}
