#include <iostream>
using namespace std;

int main() {
    int N;
    cin >> N;

    int A[100], B[100], C[100];

    for (int i = 0; i < N; i++) {
        cin >> A[i];
    }

    for (int i = 0; i < N; i++) {
        cin >> B[i];
    }

    for (int i = 0; i < N; i++) {
        C[i] = A[i] + B[i];
    }

    for (int i = 0; i < N; i++) {
        cout << C[i] << " ";
    }

    return 0;
}
