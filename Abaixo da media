#include <iostream>
using namespace std;

int main() {
    int N;
    cin >> N;

    double vet[100];

    for (int i = 0; i < N; i++) {
        cin >> vet[i];
    }

    double soma = 0;

    for (int i = 0; i < N; i++) {
        soma += vet[i];
    }

    double media = soma / N;

    cout.setf(ios::fixed);
    cout.precision(3);
    cout << "MEDIA = " << media << endl;

    cout.precision(1);
    for (int i = 0; i < N; i++) {
        if (vet[i] < media) {
            cout << vet[i] << endl;
        }
    }

    return 0;
}
