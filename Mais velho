#include <iostream>
#include <string>
using namespace std;

int main() {
    int N;
    cin >> N;

    string nomes[100];
    int idades[100];

    for (int i = 0; i < N; i++) {
        cin >> nomes[i];
        cin >> idades[i];
    }

    int maiorIdade = idades[0];
    int pos = 0;

    for (int i = 1; i < N; i++) {
        if (idades[i] > maiorIdade) {
            maiorIdade = idades[i];
            pos = i;
        }
    }

    cout << nomes[pos] << endl;

    return 0;
}
