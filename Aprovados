#include <iostream>
#include <string>
using namespace std;

int main() {
    int N;
    cin >> N;

    string nomes[100];
    double nota1[100], nota2[100];

    for (int i = 0; i < N; i++) {
        cin >> nomes[i] >> nota1[i] >> nota2[i];
    }

    for (int i = 0; i < N; i++) {
        double media = (nota1[i] + nota2[i]) / 2;
        if (media >= 6.0) {
            cout << nomes[i] << endl;
        }
    }

    return 0;
}
