#include <iostream>
using namespace std;

int main() {
    int N;
    cin >> N;

    double vet[100];

    for (int i = 0; i < N; i++) {
        cin >> vet[i];
    }

    for (int i = 0; i < N; i++) {
        cout << vet[i] << " ";
    }
    cout << endl;

    double soma = 0;
    for (int i = 0; i < N; i++) {
        soma += vet[i];
    }

    double media = soma / N;

    cout << "SOMA = " << soma << endl;
    cout << "MEDIA = " << media << endl;

    return 0;
}
